    "eslint": "9.39.1",
    "eslint-plugin-oxlint": "1.30.0",
    "eslint-plugin-vue": "10.6.2",
    "jiti": "2.6.1",
    "npm-run-all2": "8.0.4",
    "oxlint": "1.30.0",
    "sass": "1.94.2",
    "typescript": "5.9.3",
